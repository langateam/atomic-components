{
  "name": "langa.io",
  "version": "0.0.10",
  "description": "Langa Website",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node server.js",
    "lint": "eslint src",
    "test": "eslint . && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/langateam/langa.io.git"
  },
  "keywords": [
    "langa",
    "trails",
    "trailsjs"
  ],
  "author": "Joey Di Nardo",
  "license": "MIT",
  "homepage": "https://github.com/langateam/langa.io",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-trails": "^2.0.6",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.15.0",
    "node-sass": "^4.5.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^2.1.0",
    "github-fork-ribbon-css": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-sass": "^3.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-popover": "^0.4.4",
    "trailpack-gulp": "^1.0.2",
    "trailpack-hapi": "^2.2.0",
    "webpack": "^1.14.0",
    "webpack-stream": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "trails/react",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "babel": {
    "presets": [
      "react"
    ]
  }
}
